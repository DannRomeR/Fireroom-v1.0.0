        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.FireroomV100"
        tools:targetApi="31">
        <activity
            android:name=".ui.authenticate.EmailCreateAccountActivity"
            android:exported="false" />
<!--        <activity-->
<!--            android:name=".ui.authenticate.EmailSignUpActivity"-->
<!--            android:exported="false" />-->
        <activity
            android:name=".ui.authenticate.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".NavigationActivity"
            android:exported="true"
            android:label="@string/title_activity_navigation"
            android:theme="@style/Theme.FireroomV100.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
